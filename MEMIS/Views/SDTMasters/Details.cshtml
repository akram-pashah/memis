    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.ServiceDeliveryTimeline)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.ServiceDeliveryTimeline)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Measure)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Measure)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.EvaluationPeriod)
            </dt>
            <dd class="col-sm-10">

            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Target)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Target)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Numerator)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Numerator)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Denominator)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Denominator)
            </dd>
            <dt class="col-sm-2">
                Department
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.DepartmentFk.deptName)
            </dd>
        </dl>
    </div>
    <div class="card-footer">
        <a role="button" class="btn btn-sm btn-outline-primary" asp-action="Edit" asp-route-id="@Model?.Id">Edit</a> |
        <a role="button" class="btn btn-sm btn-outline-secondary" asp-action="Index">Back to List</a>
    </div>
</div>
